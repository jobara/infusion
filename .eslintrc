{
  "env": {
    "browser": true,
    "jquery": true,
    "worker": true
  },
  "globals": {
    "fluid_2_0_0": true,
    "JSON": false
  },
  "rules": {
    "no-bitwise": 0,
    "camelcase": 0,
    "curly": 2,
    "eqeqeq": 2,
    "guard-for-in": 0,
    "no-extend-native": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "no-use-before-define": 2,
    "new-cap": 2,
    "no-caller": 2,
    "no-empty": 0,
    "no-irregular-whitespace": 2,
    "no-new": 2,
    "no-plusplus": 0,
    "quotes": [
      2,
      "single"
    ],
    "no-undef": 2,
    "no-unused-vars": 2,
    "strict": [
      2,
      "function"
    ]
  }
}
